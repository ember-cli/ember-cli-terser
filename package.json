{
  "name": "ember-cli-terser",
  "version": "4.0.2",
  "description": "JavaScript minification for Ember-CLI",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://github.com/ember-cli/ember-cli-terser#readme",
  "bugs": {
    "url": "https://github.com/ember-cli/ember-cli-terser/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ember-cli/ember-cli-terser.git"
  },
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test --environment=production",
    "test:node": "mocha node-tests"
  },
  "dependencies": {
    "broccoli-terser-sourcemap": "^4.1.0"
  },
  "devDependencies": {
    "@release-it-plugins/lerna-changelog": "^5.0.0",
    "chai": "^4.3.7",
    "ember-cli": "~3.22.0",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-qunit": "^4.4.0",
    "ember-load-initializers": "^2.1.2",
    "ember-resolver": "^8.0.2",
    "ember-source": "^3.27.5",
    "loader.js": "^4.7.0",
    "mocha": "^8.4.0",
    "release-it": "^15.6.0"
  },
  "engines": {
    "node": "10.* || 12.* || >= 14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "before": "ember-cli-sri",
    "after": "broccoli-asset-rev"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    },
    "npm": {
      "publish": false
    }
  },
  "volta": {
    "node": "14.21.2",
    "yarn": "1.22.19"
  }
}
